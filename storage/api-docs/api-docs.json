{
    "openapi": "3.0.0",
    "info": {
        "title": "API RWANDA",
        "version": "1.0"
    },
    "paths": {
        "/api/password/verifemail": {
            "post": {
                "tags": [
                    "Password"
                ],
                "description": "Vérification de l'email",
                "operationId": "verifemail",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "fabio@yopmail.com"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Vérification de l'email."
                    },
                    "401": {
                        "description": "Aucune donnée trouvée."
                    },
                    "404": {
                        "description": "Page introuvable."
                    }
                }
            }
        },
        "/api/password/verifotp": {
            "post": {
                "tags": [
                    "Password"
                ],
                "description": "Vérification du Code OTP",
                "operationId": "verifotp",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "otp"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "otp": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Vérification du Code OTP."
                    },
                    "401": {
                        "description": "Aucune donnée trouvée."
                    },
                    "404": {
                        "description": "Page introuvable."
                    }
                }
            }
        },
        "/api/password/addpass": {
            "post": {
                "tags": [
                    "Password"
                ],
                "description": "Ajout de Mot de passe",
                "operationId": "addpass",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Mot de passe modifié avec succès."
                    },
                    "401": {
                        "description": "Aucune donnée trouvée."
                    },
                    "404": {
                        "description": "Page introuvable."
                    }
                }
            }
        },
        "/api/password/editpass": {
            "post": {
                "tags": [
                    "Password"
                ],
                "description": "Modification de Mot de passe",
                "operationId": "editpass",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "oldpass",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "oldpass": {
                                        "type": "string",
                                        "format": "password"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Mot de passe modifié avec succès."
                    },
                    "401": {
                        "description": "Aucune donnée trouvée."
                    },
                    "404": {
                        "description": "Page introuvable."
                    }
                },
                "security": [
                    {
                        "bearer": []
                    }
                ]
            }
        },
        "/api/users/register": {
            "post": {
                "tags": [
                    "Users"
                ],
                "description": "Account creation",
                "operationId": "register",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "lg",
                                    "lastname",
                                    "firstname",
                                    "gender",
                                    "number",
                                    "email",
                                    "birthday",
                                    "birthplace",
                                    "profession",
                                    "village",
                                    "street_number",
                                    "hourse_number",
                                    "family_number",
                                    "fullname_peson",
                                    "number_person",
                                    "residence_person",
                                    "cellule_id",
                                    "maritalstatus_id",
                                    "district_id",
                                    "lastname_father",
                                    "firstname_father",
                                    "lastname_mother",
                                    "firstname_mother",
                                    "photo"
                                ],
                                "properties": {
                                    "lg": {
                                        "type": "string"
                                    },
                                    "lastname": {
                                        "type": "string"
                                    },
                                    "firstname": {
                                        "type": "string"
                                    },
                                    "gender": {
                                        "type": "string"
                                    },
                                    "number": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "birthday": {
                                        "type": "string",
                                        "format": "date"
                                    },
                                    "birthplace": {
                                        "type": "string"
                                    },
                                    "profession": {
                                        "type": "string"
                                    },
                                    "village": {
                                        "type": "string"
                                    },
                                    "street_number": {
                                        "type": "string"
                                    },
                                    "hourse_number": {
                                        "type": "string"
                                    },
                                    "family_number": {
                                        "type": "integer"
                                    },
                                    "fullname_peson": {
                                        "type": "string"
                                    },
                                    "number_person": {
                                        "type": "string"
                                    },
                                    "residence_person": {
                                        "type": "string"
                                    },
                                    "cellule_id": {
                                        "type": "integer"
                                    },
                                    "district_id": {
                                        "type": "integer"
                                    },
                                    "bp": {
                                        "type": "string"
                                    },
                                    "diplome": {
                                        "type": "string"
                                    },
                                    "distinction": {
                                        "type": "string"
                                    },
                                    "maritalstatus_id": {
                                        "type": "integer"
                                    },
                                    "nationality_id": {
                                        "type": "integer"
                                    },
                                    "lastname_father": {
                                        "type": "string"
                                    },
                                    "firstname_father": {
                                        "type": "string"
                                    },
                                    "lastname_mother": {
                                        "type": "string"
                                    },
                                    "firstname_mother": {
                                        "type": "string"
                                    },
                                    "photo": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Création de compte éffectuée avec succès."
                    },
                    "401": {
                        "description": "Echec de Création de compte."
                    },
                    "404": {
                        "description": "Page introuvable."
                    }
                }
            }
        },
        "/api/users/auth": {
            "post": {
                "tags": [
                    "Users"
                ],
                "description": "Authenticate Platform and Generate JWT",
                "operationId": "login",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "login",
                                    "password"
                                ],
                                "properties": {
                                    "login": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Authentification éffectuée avec succès."
                    },
                    "401": {
                        "description": "Echec d'authentification."
                    },
                    "404": {
                        "description": "Page introuvable."
                    }
                }
            }
        },
        "/api/users/logout": {
            "post": {
                "tags": [
                    "Users"
                ],
                "description": "Deconnecte l'utilisateur en supprimant son token d'accès",
                "operationId": "logout",
                "responses": {
                    "200": {
                        "description": "Déconnexion éffectuée avec succès."
                    },
                    "401": {
                        "description": "Echec d'authentification."
                    },
                    "404": {
                        "description": "Page introuvable."
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Password",
            "description": "Password"
        },
        {
            "name": "Users",
            "description": "Users"
        }
    ]
